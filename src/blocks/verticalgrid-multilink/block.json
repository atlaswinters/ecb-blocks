{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ecb-blocks/verticalgridmultilink",
	"version": "0.1.0",
	"title": "Vertical Grid - MultiLink",
	"category": "ecb-blocks",
	"icon": "layout",
	"attributes": {
		"stripeBg": {
			"type": "string",
            "default": "#f1ece6"
		},
		"mainBgColor": {
			"type": "string",
        	"default": "#f7f7f4"
		},
		"photo": {
			"type": "string",
			"default": "https://images.unsplash.com/photo-1505284361246-cedb2a8d2986?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1052&q=80"
		},
		"photoTwo": {
			"type": "string",
			"default": "https://images.unsplash.com/photo-1505284361246-cedb2a8d2986?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1052&q=80"
		},
		"photoThree": {
			"type": "string",
			"default": "https://images.unsplash.com/photo-1505284361246-cedb2a8d2986?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1052&q=80"
		},
		"photoFour": {
			"type": "string",
			"default": "https://images.unsplash.com/photo-1505284361246-cedb2a8d2986?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1052&q=80"
		},
		"subheadline": {
			"type": "string",
			"default": "Subheadline extends slight longer"
		},
		"link": {
			"type": "string",
			"default": "#"
		},
		"linkText": {
			"type": "string",
			"default": "A new kind of headline"
		},
		"linkOne": {
			"type": "string",
			"default": "#"
		},
		"linkTwo": {
			"type": "string",
			"default": "#"
		},
		"linkThree": {
			"type": "string",
			"default": "#"
		},
		"linkFour": {
			"type": "string",
			"default": "#"
		}
	},
	"supports": {
		"align": [
			"wide",
			"full"
		],
		"html": false,
		"anchor": true,
		"color": {
			"background": true,
			"text": true,
			"link": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"keywords": [
		"Catalog",
		"feature",
		"highlight",
		"Layout"
	],
	"textdomain": "ecb-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}